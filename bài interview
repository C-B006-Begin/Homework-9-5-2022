#include <stdio.h>
int main()
{   int n;
int s = 0;
    printf("nhap vao chuoi ky tu cua ban :");
    scanf("%d",&n);
    for(int i = 0; i<=n;i++){
        if(i%3==0&&i%5==0)
        {
            printf("so %d la fizzbuzz\n",i);
        }
        else if(i%3==0){
            printf("so %d la fizz\n", i);
        }
        else if(i%5==0){
          printf("so %d la buzz\n",i); 
        }
    }
}
