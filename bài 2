#include <stdio.h>
#include <string.h>
int main()
{
    int count = 0;
    int n=0;
    char str[100];
    printf("nhap vao chuoi ky tu cua ban :");
    gets(str);
     while(str[count]!='\0'){
        count++;
    }
    for (int i = 0; i < count; i++){
        if(str[i]=='a'||str[i]=='e'||str[i]=='u'||str[i]=='i'||str[i]=='o'||str[i]=='A'||str[i]=='E'||str[i]=='U'||str[i]=='I'||str[i]=='O'){
            n++;
        }
    }
        printf("chuoi %s co %d nguyen am",str,n);
}
